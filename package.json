{
  "name": "omnixys-ticket-service",
  "version": "1.0.0",
  "description": "Omnixys Ticket Service",
  "author": {
    "name": "Caleb Gyamfi",
    "email": "caleb@omnixys.com",
    "url": "https://omnixys.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/omnixys/omnixys-ticket-service.git"
  },
  "bugs": {
    "url": "https://github.com/omnixys/omnixys-ticket-service/issues"
  },
  "homepage": "https://omnixys.com",
  "license": "GPL-3.0-or-later",
  "type": "module",
  "private": false,
  "engines": {
    "node": ">=24.10.0",
    "pnpm": ">=10.19.0"
  },
  "scripts": {
    "clean": "rm -rf dist tsconfig.build.tsbuildinfo",
    "build": "pnpm run clean && nest build",
    "format": "prettier --cache --write \"src/**/*.{ts,js,json,prisma,md,yaml}\"",
    "format:test": "prettier --cache --write \"src/**/*.{ts,js,json,prisma,md,yaml}\" \"__tests__/**/*.ts\"",
    "format:check": "prettier --cache --check \"src/**/*.{ts,js,json,prisma,md,yaml,yml}\" \"__test__/**/*.ts\"",
    "start": "pnpm nest start",
    "dev": "pnpm run clean && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest --config ./jest-e2e.json --maxWorkers=4 --coverage",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e:debug": "NODE_OPTIONS='--experimental-vm-modules' jest --runInBand --detectOpenHandles --config ./__tests__/jest-e2e.json",
    "test:e2e2": "jest --config ./__tests__/jest-e2e.json",
    "test:e2e": "NODE_OPTIONS='--experimental-vm-modules' jest --config ./__tests__/e2e/jest-e2e.json --passWithNoTests",
    "typedoc": "typedoc",
    "depcheck": "depcheck",
    "test:login": "pnpm test -- __tests__/e2e/ticket/ticket.login.e2e-spec.ts",
    "test:signup": "pnpm test -- __tests__/e2e/ticket/ticket.signup.e2e-spec.ts",
    "test:ticket": "pnpm test -- __tests__/e2e/ticket/ticket.ticket.e2e-spec.ts",
    "test:admin": "pnpm test -- __tests__/e2e/ticket/ticket.admin.e2e-spec.ts",
    "prepare": "husky",
    "prisma:gen": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset --force && prisma migrate dev && tsx prisma/seed.ts",
    "prisma:studio": "prisma studio --schema prisma/schema.prisma",
    "prisma:seed": "tsx prisma/seed.ts",
    "prisma:push": "prisma db push",
    "prisma:pull": "prisma db pull",
    "prisma:format": "pnpm prisma format && pnpm prisma validate && prisma generate"
  },
  "dependencies": {
    "@apollo/server": "^5.1.0",
    "@apollo/subgraph": "^2.12.1",
    "@as-integrations/fastify": "^3.1.0",
    "@fastify/compress": "^8.3.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/rate-limit": "^10.3.0",
    "@js-temporal/polyfill": "^0.5.1",
    "@nestjs/apollo": "^13.2.1",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/graphql": "^13.2.0",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-fastify": "^11.1.9",
    "@nestjs/terminus": "^11.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.67.0",
    "@opentelemetry/context-async-hooks": "^2.2.0",
    "@opentelemetry/exporter-prometheus": "^0.208.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@valkey/client": "^1.0.0",
    "cfonts": "^3.3.1",
    "chalk": "^5.6.2",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "jose": "^6.1.2",
    "jwks-rsa": "^3.2.0",
    "kafkajs": "^2.2.4",
    "passport-jwt": "^4.0.1",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "prisma": "^7.2.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@tsconfig/node23": "^23.0.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-sort-imports": "^1.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "tsx": "^4.21.0",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}